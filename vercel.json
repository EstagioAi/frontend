{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    { "source": "/mural-de-vagas", "destination": "/radar-de-vagas", "permanent": true }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/((?!.*\\..*).*)", "destination": "/index.html" }
  ]
}
