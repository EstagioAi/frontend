/* Slot Machine Animations */

/* Keyframes for vertical slot spin animation */
@keyframes slot-spin-up {
  0% {
    transform: translate3d(0, 0, 0);
    opacity: 1;
  }
  100% {
    transform: translate3d(0, -100%, 0);
    opacity: 0.5;
  }
}

/* Slot Machine Container Styles */
.slot-machine-container {
  position: relative;
  background: rgba(255, 255, 255, 0.95);
  border: 1px solid rgba(217, 119, 87, 0.2);
  border-radius: 1rem;
  padding: 1rem 1.5rem;
  box-shadow: 0 4px 12px rgba(217, 119, 87, 0.15);
  backdrop-filter: blur(8px);
  max-width: 220px;
  height: 60px;
  overflow: hidden;
  transition: all 0.3s ease;
  cursor: pointer;
}

.slot-machine-container:hover {
  box-shadow: 0 8px 24px rgba(217, 119, 87, 0.25);
  transform: scale(1.03);
  border-color: rgba(217, 119, 87, 0.4);
}

.slot-machine-inner {
  display: flex;
  align-items: center;
  height: 100%;
  gap: 0.75rem;
}

.slot-icon {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  color: #d97757;
}

.slot-reel-container {
  position: relative;
  flex: 1;
  height: 100%;
  overflow: hidden;
}

.slot-reel {
  display: flex;
  flex-direction: column;
  transition: transform 0.6s cubic-bezier(0.4, 0, 0.2, 1);
  will-change: transform;
}

/* Apply GPU acceleration */
.slot-reel {
  transform: translate3d(0, 0, 0);
}

/* Remove will-change when paused for performance */
.slot-machine-container:hover .slot-reel {
  will-change: auto;
}

.slot-item {
  font-size: 1.25rem;
  font-weight: 600;
  color: #3d3d3a;
  white-space: nowrap;
  line-height: 60px;
  height: 60px;
  text-align: left;
  flex-shrink: 0;
}

/* Gradient overlays for fade effect */
.slot-reel-container::before,
.slot-reel-container::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 15px;
  pointer-events: none;
  z-index: 10;
}

.slot-reel-container::before {
  top: 0;
  background: linear-gradient(to bottom, rgba(255, 255, 255, 0.95), transparent);
}

.slot-reel-container::after {
  bottom: 0;
  background: linear-gradient(to top, rgba(255, 255, 255, 0.95), transparent);
}

