/* Custom Animations - Design System
   Performance optimized: Using only transform and opacity (GPU-accelerated)
   All animations respect prefers-reduced-motion */

/* Float Animation - for floating cards */
@keyframes float {
  0%, 100% { 
    transform: translateY(0) translateZ(0); 
  }
  50% { 
    transform: translateY(-10px) translateZ(0); 
  }
}

/* Fade In Up - for scroll animations */
@keyframes fade-in-up {
  from {
    opacity: 0;
    transform: translateY(20px) translateZ(0);
  }
  to {
    opacity: 1;
    transform: translateY(0) translateZ(0);
  }
}

/* Blog Card Entrance Animation - Fade in up with stagger
   Used for blog post cards on page load
   Respects prefers-reduced-motion */
@keyframes blog-card-entrance {
  from {
    opacity: 0;
    transform: translateY(20px) translateZ(0);
  }
  to {
    opacity: 1;
    transform: translateY(0) translateZ(0);
  }
}

/* Scale In - for entrance animations */
@keyframes scale-in {
  from {
    opacity: 0;
    transform: scale(0.95) translateZ(0);
  }
  to {
    opacity: 1;
    transform: scale(1) translateZ(0);
  }
}

/* Additional Animations */
@keyframes morphing {
  0%, 100% { border-radius: 60% 40% 30% 70% / 60% 30% 70% 40%; }
  50% { border-radius: 30% 60% 70% 40% / 50% 60% 30% 60%; }
}

@keyframes float-delayed {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(-15px) translateZ(0); }
}

@keyframes float-reverse {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(15px) translateZ(0); }
}

@keyframes pulse-glow {
  0%, 100% { 
    opacity: 0.4; 
    transform: scale(1) translateZ(0); 
  }
  50% { 
    opacity: 0.8; 
    transform: scale(1.1) translateZ(0); 
  }
}

@keyframes rotate-slow {
  from { transform: rotate(0deg) translateZ(0); }
  to { transform: rotate(360deg) translateZ(0); }
}

@keyframes bounce-subtle {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(-5px) translateZ(0); }
}

@keyframes gradient-shift {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

@keyframes wiggle {
  0%, 100% { transform: rotate(0deg) translateZ(0); }
  25% { transform: rotate(-3deg) translateZ(0); }
  75% { transform: rotate(3deg) translateZ(0); }
}

@keyframes slow-float {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(-8px) translateZ(0); }
}

@keyframes slow-float-delayed {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(10px) translateZ(0); }
}

@keyframes bounce {
  0%, 100% { transform: translateY(0) translateZ(0); }
  50% { transform: translateY(-10px) translateZ(0); }
}

