@import "tailwindcss";
@import "tw-animate-css";

@custom-variant dark (&:is(.dark *));

@theme inline {
  --radius-sm: calc(var(--radius) - 4px);
  --radius-md: calc(var(--radius) - 2px);
  --radius-lg: var(--radius);
  --radius-xl: calc(var(--radius) + 4px);
  --color-background: var(--background);
  --color-foreground: var(--foreground);
  --color-card: var(--card);
  --color-card-foreground: var(--card-foreground);
  --color-popover: var(--popover);
  --color-popover-foreground: var(--popover-foreground);
  --color-primary: var(--primary);
  --color-primary-foreground: var(--primary-foreground);
  --color-secondary: var(--secondary);
  --color-secondary-foreground: var(--secondary-foreground);
  --color-muted: var(--muted);
  --color-muted-foreground: var(--muted-foreground);
  --color-accent: var(--accent);
  --color-accent-foreground: var(--accent-foreground);
  --color-destructive: var(--destructive);
  --color-border: var(--border);
  --color-input: var(--input);
  --color-ring: var(--ring);
}

:root {
  /* ========================================
     ESTAGIOAI DESIGN SYSTEM - PALETA VERDE E PRETA
     ======================================== */

  /* Shadcn/UI Base Variables */
  --background: #ffffff;
  --foreground: #0f0f0f;
  --card: #ffffff;
  --card-foreground: #0f0f0f;
  --popover: #ffffff;
  --popover-foreground: #0f0f0f;
  --primary: #82f7b3;
  --primary-foreground: #0f0f0f;
  --secondary: #f8f8f8;
  --secondary-foreground: #0f0f0f;
  --muted: #f8f8f8;
  --muted-foreground: rgba(15, 15, 15, 0.7);
  --accent: #82f7b3;
  --accent-foreground: #0f0f0f;
  --destructive: #ef4444;
  --border: rgba(15, 15, 15, 0.1);
  --input: rgba(15, 15, 15, 0.1);
  --ring: #82f7b3;
  --radius: 1rem;

  /* ========================================
     CORES PRINCIPAIS - Verde e Preto
     ======================================== */
  --color-primary-green: #82f7b3;
  --color-primary-green-hover: #6ee5a1;
  --color-primary-black: #0f0f0f;
  --color-primary-black-hover: #1a1a1a;

  /* Backgrounds */
  --color-bg-primary: #f8f8f8;
  --color-bg-secondary: #f5f4ed;
  --color-bg-white: #ffffff;

  /* Accent Colors */
  --color-accent-primary: #82f7b3;
  --color-accent-hover: #6ee5a1;

  /* Text Colors - Preto com opacidades (Hierarquia tipográfica)
     Uso: Textos com diferentes níveis de importância visual
     Hierarquia: 100% (títulos) > 70% (corpo) > 50% (secundário) > 40% (terciário) > 30% (auxiliar) > 10% (bordas) */
  --color-text-primary: #0f0f0f;       /* Preto 100% - títulos, texto principal */
  --color-text-secondary: rgba(15, 15, 15, 0.7);  /* Preto 70% - texto de corpo, descrições */
  --color-text-tertiary: rgba(15, 15, 15, 0.5);   /* Preto 50% - texto auxiliar, labels */
  --color-text-muted: rgba(15, 15, 15, 0.4);      /* Preto 40% - placeholders, texto desabilitado */
  --color-text-subtle: rgba(15, 15, 15, 0.3);     /* Preto 30% - metadados, timestamps */

  /* Border Colors - Preto com opacidades (Bordas sutis)
     Uso: Bordas de cards, inputs, separadores
     Padrão: 10% para bordas sutis, 20% para bordas mais visíveis */
  --color-border-light: rgba(15, 15, 15, 0.1);    /* Preto 10% - bordas sutis, separadores */
  --color-border-medium: rgba(15, 15, 15, 0.2);   /* Preto 20% - bordas visíveis, elementos ativos */

  /* Green Accent Colors - Verde com opacidades (Destaques e interações)
     Uso: Backgrounds de hover, focus rings, elementos interativos */
  --color-green-bg-light: rgba(130, 247, 179, 0.1);  /* Verde 10% - backgrounds sutis */
  --color-green-bg-medium: rgba(130, 247, 179, 0.2); /* Verde 20% - focus rings */
  --color-green-bg-strong: rgba(130, 247, 179, 0.3); /* Verde 30% - elementos decorativos */

  /* Sistema de Opacidades Padronizadas
     Uso: Aplicar consistentemente sobre cores base
     Escala: 70%, 50%, 40%, 30%, 10% */
  --opacity-text-secondary: 0.7;       /* 70% - texto secundário */
  --opacity-text-tertiary: 0.5;        /* 50% - texto auxiliar */
  --opacity-text-muted: 0.4;           /* 40% - placeholders */
  --opacity-text-subtle: 0.3;          /* 30% - metadados */
  --opacity-border: 0.1;               /* 10% - bordas sutis */
  --opacity-border-medium: 0.2;        /* 20% - bordas visíveis */

  /* Legacy Colors - Mantidos para compatibilidade gradual */
  --color-success: #01E297;            /* Verde menta - mantido para compatibilidade */
  --color-info: #60a5fa;               /* Azul suave - mantido para compatibilidade */
  --color-warning: #fbbf24;            /* Amarelo - mantido para compatibilidade */

  /* Legacy Brand Colors - Mantidos para compatibilidade */
  --brand-green: #01E297;
  --brand-blue: #2563eb;
  --brand-dark: #0f172a;

  /* Legacy Background Colors - Mantidos para compatibilidade */
  --bg-blue-light: #eff6ff;
  --bg-pink-light: #fdf2f8;
  --bg-green-light: #f0fdf4;
  --bg-purple-light: #faf5ff;

  /* Semantic Text Colors - Nova nomenclatura verde/preta
     Uso: Aplicar cores de texto de forma consistente e semântica
     Hierarquia visual: primary (títulos) > secondary (corpo) > muted (auxiliar) */
  --text-primary: var(--color-text-primary);     /* Texto principal - títulos, headings, conteúdo importante */
  --text-secondary: var(--color-text-secondary); /* Texto secundário - corpo, parágrafos, descrições */
  --text-muted: var(--color-text-tertiary);      /* Texto auxiliar - labels, placeholders, metadados */
  --text-green: var(--color-primary-green);      /* Texto verde - links, CTAs, elementos interativos */

  /* Semantic UI Colors - Nova nomenclatura verde/preta
     Uso: Cores pré-configuradas para elementos de UI comuns
     Facilita consistência e manutenção do design system */
  --border-light: var(--color-border-light);                    /* Borda neutra clara - dividers, separadores */
  --border-green: var(--color-green-bg-light);                  /* Borda verde suave - cards, containers */
  --border-green-strong: var(--color-green-bg-medium);          /* Borda verde visível - elementos ativos */
  --shadow-sm: rgba(0, 0, 0, 0.05);                            /* Sombra mínima - hover states */
  --shadow-md: rgba(0, 0, 0, 0.1);                             /* Sombra média - cards padrão */
  --shadow-lg: rgba(0, 0, 0, 0.15);                            /* Sombra grande - cards destacados */
  --shadow-green: rgba(130, 247, 179, 0.25);                   /* Sombra verde - botões, elementos verdes */
  --shadow-green-strong: rgba(130, 247, 179, 0.35);            /* Sombra verde forte - elementos ativos */

  /* Typography - Font Sizes
     Uso: Escala tipográfica consistente baseada em rem (16px base)
     Preferir clamp() para responsividade fluida em títulos
     Hierarquia: xs (labels) < sm (corpo pequeno) < base (corpo) < lg-7xl (títulos) */
  --text-xs: 0.75rem;      /* 12px - Labels pequenos, metadados, timestamps */
  --text-sm: 0.875rem;     /* 14px - Texto secundário, captions, helper text */
  --text-base: 1rem;       /* 16px - Texto de corpo padrão, parágrafos */
  --text-lg: 1.125rem;     /* 18px - Texto destacado, leads, subtítulos pequenos */
  --text-xl: 1.25rem;      /* 20px - Subtítulos, H4 */
  --text-2xl: 1.5rem;      /* 24px - H3, títulos de seção */
  --text-3xl: 1.875rem;    /* 30px - H2, títulos de página */
  --text-4xl: 2.25rem;     /* 36px - H1 pequeno, títulos principais */
  --text-5xl: 3rem;        /* 48px - H1 médio, hero titles */
  --text-6xl: 3.75rem;     /* 60px - H1 grande, display titles */
  --text-7xl: 4.5rem;      /* 72px - Display extra large, landing pages */

  /* Font Weights
     Uso: Hierarquia de peso para criar contraste visual
     Space Grotesk suporta 400-800, usar bold+ para títulos */
  --font-normal: 400;      /* Texto de corpo, parágrafos, descrições */
  --font-medium: 500;      /* Ênfase leve, labels, botões secundários */
  --font-semibold: 600;    /* Subtítulos, H3-H4, botões primários */
  --font-bold: 700;        /* Títulos H2, elementos de destaque */
  --font-extrabold: 800;   /* Títulos H1, hero titles, máximo impacto */

  /* Line Heights
     Uso: Legibilidade e espaçamento vertical
     Regra: títulos (tight) < corpo (normal) < texto longo (relaxed) */
  --leading-tight: 1.2;    /* Títulos, headlines - compacto para impacto */
  --leading-normal: 1.5;   /* Texto de corpo padrão - legibilidade ideal */
  --leading-relaxed: 1.75; /* Texto longo, artigos - conforto de leitura */

  /* Spacing System (8px base)
     Uso: Sistema de espaçamento consistente baseado em múltiplos de 8px
     Aplicar em padding, margin, gap para manter ritmo vertical
     Escala: 1 (4px) < 2 (8px) < 3 (12px) < 4 (16px) ... 16 (128px) */
  --space-1: 0.5rem;       /* 8px - Espaçamento mínimo, padding interno pequeno */
  --space-2: 1rem;         /* 16px - Espaçamento padrão entre elementos próximos */
  --space-3: 1.5rem;       /* 24px - Espaçamento médio, gap entre componentes */
  --space-4: 2rem;         /* 32px - Espaçamento entre seções pequenas */
  --space-5: 2.5rem;       /* 40px - Espaçamento entre seções médias */
  --space-6: 3rem;         /* 48px - Espaçamento entre seções grandes */
  --space-8: 4rem;         /* 64px - Espaçamento entre seções principais */
  --space-10: 5rem;        /* 80px - Padding vertical de seções */
  --space-12: 6rem;        /* 96px - Padding vertical de seções grandes */
  --space-16: 8rem;        /* 128px - Espaçamento máximo, hero sections */

  /* Border Radius
     Uso: Arredondamento de cantos para suavidade visual
     Padrão: cards (lg/xl), botões (lg), badges (full), inputs (md) */
  --radius-sm: 0.5rem;     /* 8px - Elementos pequenos, badges internos */
  --radius-md: 0.75rem;    /* 12px - Inputs, selects, elementos de formulário */
  --radius-lg: 1rem;       /* 16px - Botões, cards pequenos */
  --radius-xl: 1.25rem;    /* 20px - Cards médios, containers */
  --radius-2xl: 1.5rem;    /* 24px - Cards grandes, feature cards */
  --radius-full: 9999px;   /* Circular - badges, avatares, pills */

  /* Shadows
     Uso: Elevação e profundidade de elementos
     Hierarquia: sm (hover) < md (cards) < lg (modais) < xl (dropdowns) < 2xl (overlays) */
  --shadow-sm: 0 1px 2px rgba(0, 0, 0, 0.05);      /* Elevação mínima - hover states */
  --shadow-md: 0 4px 6px rgba(0, 0, 0, 0.07);      /* Elevação padrão - cards, botões */
  --shadow-lg: 0 10px 15px rgba(0, 0, 0, 0.1);     /* Elevação média - cards destacados */
  --shadow-xl: 0 20px 25px rgba(0, 0, 0, 0.15);    /* Elevação alta - modais, dropdowns */
  --shadow-2xl: 0 25px 50px rgba(0, 0, 0, 0.25);   /* Elevação máxima - overlays, popovers */

  /* Animation Durations - Sistema de transições rápidas (200ms padrão)
     Uso: Duração de transições e animações otimizadas
     Foco em transições rápidas para responsividade */
  --duration-fast: 200ms;    /* Transições padrão - hover, focus, micro-interações */
  --duration-medium: 300ms;  /* Transições médias - transformações, scale */
  --duration-slow: 500ms;    /* Transições complexas - modais, page transitions */

  /* Animation Easings - Curvas otimizadas para o novo design
     Uso: Curvas de aceleração para animações naturais e rápidas */
  --ease-in-out: cubic-bezier(0.4, 0, 0.2, 1);  /* Aceleração suave - transições padrão */
  --ease-out: cubic-bezier(0, 0, 0.2, 1);       /* Desaceleração - entrada de elementos */
  --ease-in: cubic-bezier(0.4, 0, 1, 1);        /* Aceleração - saída de elementos */

  /* Transition Properties - Propriedades de transição padrão
     Uso: Aplicar transições consistentes em elementos interativos */
  --transition-all: all var(--duration-fast) var(--ease-in-out);           /* Transição completa - elementos complexos */
  --transition-colors: color var(--duration-fast) var(--ease-in-out),      /* Transição de cores */
                       background-color var(--duration-fast) var(--ease-in-out),
                       border-color var(--duration-fast) var(--ease-in-out);
  --transition-transform: transform var(--duration-fast) var(--ease-in-out); /* Transição de transformações */
  --transition-shadow: box-shadow var(--duration-fast) var(--ease-in-out);   /* Transição de sombras */

  /* Hover Transform Values - Valores de transformação para hover
     Uso: Elevação e escala consistentes em elementos interativos */
  --hover-translate-y: -4px;     /* Elevação padrão para hover */
  --hover-scale: 1.05;           /* Escala padrão para hover em imagens */
  --hover-scale-button: 1.02;    /* Escala sutil para botões */

  /* Breakpoints
     Uso: Media queries responsivas
     Mobile-first: estilos base para mobile, media queries para telas maiores */
  --screen-sm: 640px;    /* Small - Tablets pequenos, phones landscape */
  --screen-md: 768px;    /* Medium - Tablets */
  --screen-lg: 1024px;   /* Large - Laptops, desktops pequenos */
  --screen-xl: 1280px;   /* Extra Large - Desktops */
  --screen-2xl: 1536px;  /* 2X Large - Desktops grandes, monitores wide */

  /* ========================================
     DESIGN SYSTEM - VERDE MINIMALISTA
     Variáveis específicas para o novo design verde e preto
     ======================================== */

  /* Variáveis adicionais para opacidades específicas */
  --color-text-80: rgba(15, 15, 15, 0.8);      /* Preto 80% - texto enfatizado */
  --color-text-60: rgba(15, 15, 15, 0.6);      /* Preto 60% - texto médio */
  --color-text-15: rgba(15, 15, 15, 0.15);     /* Preto 15% - bordas médias */
  --color-text-08: rgba(15, 15, 15, 0.08);     /* Preto 8% - bordas muito sutis */
  --color-text-05: rgba(15, 15, 15, 0.05);     /* Preto 5% - backgrounds sutis */
  --color-text-25: rgba(15, 15, 15, 0.25);     /* Preto 25% - elementos auxiliares */
  --color-text-35: rgba(15, 15, 15, 0.35);     /* Preto 35% - texto muito sutil */
  --color-text-75: rgba(15, 15, 15, 0.75);     /* Preto 75% - texto de corpo */

  /* Verde com opacidades específicas */
  --color-green-05: rgba(130, 247, 179, 0.05); /* Verde 5% - backgrounds muito sutis */
  --color-green-08: rgba(130, 247, 179, 0.08); /* Verde 8% - backgrounds sutis */
  --color-green-15: rgba(130, 247, 179, 0.15); /* Verde 15% - bordas e backgrounds */
  --color-green-18: rgba(130, 247, 179, 0.18); /* Verde 18% - decorações */
  --color-green-25: rgba(130, 247, 179, 0.25); /* Verde 25% - elementos médios */
  --color-green-30: rgba(130, 247, 179, 0.30); /* Verde 30% - bordas visíveis */
  --color-green-35: rgba(130, 247, 179, 0.35); /* Verde 35% - bordas destacadas */
  --color-green-40: rgba(130, 247, 179, 0.40); /* Verde 40% - elementos ativos */
  --color-green-45: rgba(130, 247, 179, 0.45); /* Verde 45% - decorações médias */
  --color-green-55: rgba(130, 247, 179, 0.55); /* Verde 55% - decorações fortes */
  --color-green-70: rgba(130, 247, 179, 0.70); /* Verde 70% - elementos destacados */
  --color-green-75: rgba(130, 247, 179, 0.75); /* Verde 75% - decorações principais */
  --color-green-85: rgba(130, 247, 179, 0.85); /* Verde 85% - elementos muito visíveis */
  --color-green-90: rgba(130, 247, 179, 0.90); /* Verde 90% - quase sólido */
  --color-green-95: rgba(130, 247, 179, 0.95); /* Verde 95% - praticamente sólido */

  /* Outras cores com opacidades */
  --color-white-05: rgba(255, 255, 255, 0.05); /* Branco 5% - backgrounds sobre escuro */
  --color-white-10: rgba(255, 255, 255, 0.10); /* Branco 10% - backgrounds sobre escuro */
  --color-white-25: rgba(255, 255, 255, 0.25); /* Branco 25% - bordas sobre escuro */
  --color-white-40: rgba(255, 255, 255, 0.40); /* Branco 40% - elementos sobre escuro */
  --color-white-70: rgba(255, 255, 255, 0.70); /* Branco 70% - texto sobre escuro */
  --color-white-80: rgba(255, 255, 255, 0.80); /* Branco 80% - texto sobre escuro */

  /* Cores específicas para decorações */
  --color-accent-dark: rgba(17, 94, 89, 0.2);  /* Verde escuro para acentos */
  --color-accent-dark-30: rgba(17, 94, 89, 0.3); /* Verde escuro 30% */
  --color-slate: rgba(203, 213, 225, 0.5);     /* Cinza azulado para decorações */
  --color-slate-45: rgba(203, 213, 225, 0.45); /* Cinza azulado 45% */
  --color-slate-55: rgba(203, 213, 225, 0.55); /* Cinza azulado 55% */

  /* Card Border Radius - Bordas arredondadas modernas
     Uso: Cards, containers de conteúdo, elementos principais */
  --card-radius: 24px;           /* Cards padrão - bordas arredondadas (rounded-2xl) */
  --card-radius-large: 32px;     /* Cards grandes e hero elements - máximo impacto */

  /* Card Shadows - Sombras com verde sutil
     Uso: Elevação de cards, estados hover, elementos flutuantes */
  --card-shadow: 0 10px 15px rgba(0, 0, 0, 0.1);                    /* Sombra padrão - cards em repouso */
  --card-shadow-hover: 0 20px 25px rgba(0, 0, 0, 0.15);             /* Sombra hover - elevação aumentada */
  --card-shadow-green: 0 10px 15px rgba(130, 247, 179, 0.2);        /* Sombra verde - cards com destaque */
  --card-shadow-green-hover: 0 20px 25px rgba(130, 247, 179, 0.3);  /* Sombra verde hover - elementos ativos */

  /* Dark Backgrounds - Fundos escuros minimalistas
     Uso: Contraste, overlays, elementos de destaque */
  --dark-bg: var(--color-primary-black);                    /* Background escuro principal */
  --dark-overlay: rgba(15, 15, 15, 0.95);                   /* Overlay escuro sobre imagens */
  --dark-overlay-medium: rgba(15, 15, 15, 0.7);             /* Overlay médio */

  /* Green Glow Effects - Efeitos de brilho verde
     Uso: Featured elements, elementos interativos, estados hover */
  --green-glow: rgba(130, 247, 179, 0.4);                   /* Glow verde - featured */
  --green-glow-strong: rgba(130, 247, 179, 0.6);            /* Glow verde forte - hover states */
  --green-glow-subtle: rgba(130, 247, 179, 0.2);            /* Glow verde sutil - elementos decorativos */

  /* Glass Effects - Efeitos de vidro com backdrop-blur
     Uso: Navbar, badges sobre imagens, elementos flutuantes */
  --glass-bg: rgba(255, 255, 255, 0.8);                     /* Background glass padrão */
  --glass-bg-strong: rgba(255, 255, 255, 0.95);             /* Background glass forte */
  --glass-border: rgba(15, 15, 15, 0.1);                    /* Border glass sutil */
  --glass-border-strong: rgba(15, 15, 15, 0.2);             /* Border glass visível */

  /* Card Backgrounds - Fundos de cards e containers
     Uso: Cards, containers de conteúdo, elementos destacados */
  --card-bg: var(--color-bg-white);                         /* Background card padrão */
  --card-border: var(--color-border-light);                 /* Border card sutil */
  --card-border-hover: var(--color-primary-green);          /* Border card hover - verde */

  /* Spacing System - Espaçamentos do design system
     Uso: Gaps entre elementos, seções, layout */
  --section-gap: 80px;          /* Gap entre seções principais (5rem / py-20) */
  --card-gap: 24px;             /* Gap entre cards no grid (1.5rem / gap-6) */
  --content-gap: 32px;          /* Gap entre elementos de conteúdo (2rem / gap-8) */
  --element-gap: 48px;          /* Gap entre elementos grandes (3rem / gap-12) */

  /* Overlay Gradients - Gradientes para overlays
     Uso: Overlays sobre imagens, backgrounds de contraste */
  --overlay-gradient: linear-gradient(to top, rgba(15, 15, 15, 0.95) 0%, rgba(15, 15, 15, 0.7) 50%, transparent 100%);
  --overlay-gradient-strong: linear-gradient(to top, rgba(15, 15, 15, 0.98) 0%, rgba(15, 15, 15, 0.85) 60%, transparent 100%);

  /* Progress Bar - Barra de progresso verde
     Uso: Barra de progresso, loading states */
  --progress-bg: rgba(255, 255, 255, 0.3);                  /* Background da barra */
  --progress-gradient: linear-gradient(to right, var(--color-primary-green), var(--color-primary-green-hover)); /* Gradiente verde */

  /* Filter Pills - Pills de filtro modernos
     Uso: Filtros, estados active/inactive, badges */
  --pill-inactive-bg: rgba(255, 255, 255, 0.8);             /* Background pill inativo */
  --pill-inactive-border: var(--color-border-light);        /* Border pill inativo */
  --pill-active-bg: var(--color-primary-green);             /* Background pill ativo - verde */
  --pill-active-color: var(--color-primary-black);          /* Cor texto pill ativo - preto */
}

/* ========================================
   UTILITY CLASSES - VERDE MINIMALISTA
   Classes utilitárias para uso consistente
   ======================================== */

/* ========================================
   FORM ELEMENTS - PADRONIZAÇÃO
   Classes padronizadas para inputs, selects e form elements
   ======================================== */

/* Input Base Classes - Padronização de inputs e selects */
.input-base {
  width: 100%;
  border-radius: var(--radius-lg);
  border: 2px solid var(--color-border-light);
  background-color: var(--color-bg-white);
  padding: 0.75rem 1rem; /* py-3 px-4 */
  font-size: var(--text-base);
  color: var(--color-text-primary);
  outline: none;
  transition: var(--transition-colors);
  min-height: 44px; /* Touch target accessibility */
}

.input-base::placeholder {
  color: var(--color-text-muted);
}

.input-base:focus {
  border-color: var(--color-primary-green);
  box-shadow: 0 0 0 4px rgba(130, 247, 179, 0.2);
}

.input-base:disabled {
  background-color: var(--color-bg-primary);
  color: var(--color-text-muted);
  cursor: not-allowed;
}

/* Select Specific Styles */
.select-base {
  width: 100%;
  border-radius: var(--radius-lg);
  border: 2px solid var(--color-border-light);
  background-color: var(--color-bg-white);
  padding: 0.75rem 1rem;
  font-size: var(--text-base);
  color: var(--color-text-primary);
  outline: none;
  transition: var(--transition-colors);
  min-height: 44px;
  cursor: pointer;
  background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' fill='none' viewBox='0 0 20 20'%3e%3cpath stroke='%236b7280' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.5' d='m6 8 4 4 4-4'/%3e%3c/svg%3e");
  background-position: right 0.75rem center;
  background-repeat: no-repeat;
  background-size: 1.25rem 1.25rem;
  padding-right: 2.5rem;
}

.select-base:focus {
