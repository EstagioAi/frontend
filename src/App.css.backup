/* ========================================
   BUTTON UTILITIES - PADRONIZAÇÃO
   Classes utilitárias para botões consistentes
   ======================================== */

/* Button Base */
.btn-base {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  gap: 0.5rem;
  font-weight: var(--font-bold);
  border-radius: var(--radius-lg);
  transition: var(--transition-all);
  outline: none;
  cursor: pointer;
  border: 2px solid transparent;
  min-height: 44px;
  text-decoration: none;
}

.btn-base:focus-visible {
  box-shadow: 0 0 0 4px rgba(130, 247, 179, 0.2);
}

.btn-base:disabled {
  opacity: 0.5;
  cursor: not-allowed;
  pointer-events: none;
}

/* Button Sizes */
.btn-sm {
  padding: 0.5rem 1rem;
  font-size: var(--text-sm);
  min-height: 40px;
}

.btn-md {
  padding: 0.75rem 1.5rem;
  font-size: var(--text-base);
  min-height: 44px;
}

.btn-lg {
  padding: 1rem 2rem;
  font-size: var(--text-lg);
  min-height: 48px;
}

/* Button Variants */
.btn-primary {
  background-color: var(--color-primary-black);
  color: white;
}

.btn-primary:hover {
  background-color: var(--color-primary-black-hover);
  transform: translateY(-2px);
  box-shadow: var(--shadow-lg);
}

.btn-secondary {
  background-color: var(--color-primary-green);
  color: var(--color-primary-black);
}

.btn-secondary:hover {
  background-color: var(--color-primary-green-hover);
  transform: translateY(-2px);
  box-shadow: var(--shadow-lg);
}

.btn-outline {
  background-color: transparent;
  color: var(--color-text-secondary);
  border-color: var(--color-border-light);
}

.btn-outline:hover {
  background-color: var(--color-green-bg-light);
  border-color: var(--color-primary-green);
  color: var(--color-text-primary);
}

.btn-ghost {
  background-color: transparent;
  color: var(--color-text-secondary);
}

.btn-ghost:hover {
  background-color: var(--color-green-bg-light);
  color: var(--color-text-primary);
}

/* ========================================
   CARD UTILITIES - PADRONIZAÇÃO
   Classes utilitárias para cards consistentes
   ======================================== */

.card-base {
  background-color: var(--color-bg-white);
  border: 2px solid var(--color-border-light);
  border-radius: var(--radius-xl);
  padding: var(--space-6);
  transition: var(--transition-all);
  box-shadow: var(--shadow-md);
}

.card-base:hover {
  transform: translateY(-4px);
  border-color: var(--color-primary-green);
  box-shadow: var(--shadow-lg);
}

.card-compact {
  padding: var(--space-4);
}

.card-spacious {
  padding: var(--space-8);
}

/* ========================================
   LAYOUT UTILITIES - PADRONIZAÇÃO
   Classes utilitárias para layouts consistentes
   ======================================== */

/* Container Utilities */
.container-sm { max-width: 640px; margin: 0 auto; padding: 0 1rem; }
.container-md { max-width: 768px; margin: 0 auto; padding: 0 1rem; }
.container-lg { max-width: 1024px; margin: 0 auto; padding: 0 1rem; }
.container-xl { max-width: 1280px; margin: 0 auto; padding: 0 1rem; }
.container-2xl { max-width: 1536px; margin: 0 auto; padding: 0 1rem; }
.container-full { max-width: 100%; margin: 0 auto; padding: 0 1rem; }

/* Section Utilities */
.section-padding { padding: var(--section-gap) 0; }
.section-padding-sm { padding: calc(var(--section-gap) * 0.5) 0; }
.section-padding-lg { padding: calc(var(--section-gap) * 1.5) 0; }

/* Grid Utilities */
.grid-auto { display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); }
.grid-responsive { display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); }
.grid-cards { display: grid; grid-template-columns: repeat(auto-fill, minmax(280px, 1fr)); gap: var(--card-gap); }

/* ========================================
   TYPOGRAPHY SYSTEM - VERDE MINIMALISTA
   Sistema de tipografia hierárquica
   ======================================== */

/* Typography Hierarchy - H1, H2, H3, H4
   Uso: Hierarquia de títulos consistente e responsiva
   Baseado nos requirements 3.1, 3.2, 3.3, 3.4 */

/* H1 - Hero Titles (text-5xl sm:text-6xl lg:text-7xl font-bold leading-[1.1] tracking-tight) */
.typography-h1,
.h1-hero {
  font-size: 3rem; /* 48px - text-5xl */
  font-weight: 700; /* font-bold */
  line-height: 1.1; /* leading-[1.1] */
  letter-spacing: -0.025em; /* tracking-tight */
  color: var(--color-text-primary);
}

@media (min-width: 640px) {
  .typography-h1,
  .h1-hero {
    font-size: 3.75rem; /* 60px - sm:text-6xl */
  }
}

@media (min-width: 1024px) {
  .typography-h1,
  .h1-hero {
    font-size: 4.5rem; /* 72px - lg:text-7xl */
  }
}

/* H2 - Section Titles (text-3xl sm:text-4xl font-bold tracking-tight) */
.typography-h2,
.h2-section {
  font-size: 1.875rem; /* 30px - text-3xl */
  font-weight: 700; /* font-bold */
  line-height: 1.2; /* leading-tight */
  letter-spacing: -0.025em; /* tracking-tight */
  color: var(--color-text-primary);
}

@media (min-width: 640px) {
  .typography-h2,
  .h2-section {
    font-size: 2.25rem; /* 36px - sm:text-4xl */
  }
}

/* H3 - Card Titles (text-2xl font-bold) */
.typography-h3,
.h3-card {
  font-size: 1.5rem; /* 24px - text-2xl */
  font-weight: 700; /* font-bold */
  line-height: 1.3; /* leading-snug */
  color: var(--color-text-primary);
}

/* H4 - Subtitles (text-lg sm:text-xl font-bold leading-tight) */
.typography-h4,
.h4-subtitle {
  font-size: 1.125rem; /* 18px - text-lg */
  font-weight: 700; /* font-bold */
  line-height: 1.25; /* leading-tight */
  color: var(--color-text-primary);
}

@media (min-width: 640px) {
  .typography-h4,
  .h4-subtitle {
    font-size: 1.25rem; /* 20px - sm:text-xl */
  }
}

/* Text and Paragraph Styles
   Uso: Estilos de texto e parágrafos consistentes
   Baseado nos requirements 3.5, 3.6, 3.7 */

/* Main Paragraph (text-lg leading-relaxed text-[#0f0f0f]/70) */
.typography-body,
.text-body-main {
  font-size: 1.125rem; /* 18px - text-lg */
  line-height: 1.75; /* leading-relaxed */
  color: var(--color-text-secondary); /* text-[#0f0f0f]/70 */
}

/* Secondary Text (text-sm text-[#0f0f0f]/70) */
.typography-secondary,
.text-secondary {
  font-size: 0.875rem; /* 14px - text-sm */
  line-height: 1.5; /* leading-normal */
  color: var(--color-text-secondary); /* text-[#0f0f0f]/70 */
}

/* Labels/Stats (text-xs font-semibold uppercase tracking-wider text-[#0f0f0f]/50) */
.typography-label,
.text-label,
.text-stats {
  font-size: 0.75rem; /* 12px - text-xs */
  font-weight: 600; /* font-semibold */
  text-transform: uppercase;
  letter-spacing: 0.1em; /* tracking-wider */
  color: var(--color-text-tertiary); /* text-[#0f0f0f]/50 */
}

/* Text Highlights with Green
   Uso: Destaques de texto com cor verde */
.text-highlight-green {
  color: var(--color-primary-green);
  font-weight: 600;
}

.text-highlight-green-bg {
  background: linear-gradient(120deg, rgba(130, 247, 179, 0.2) 0%, rgba(130, 247, 179, 0.1) 100%);
  padding: 0.125rem 0.25rem;
  border-radius: 0.25rem;
  color: var(--color-text-primary);
  font-weight: 600;
}

/* Utility Classes for Typography */
.text-primary {
  color: var(--color-text-primary);
}

.text-secondary-color {
  color: var(--color-text-secondary);
}

.text-tertiary {
  color: var(--color-text-tertiary);
}

.text-muted-color {
  color: var(--color-text-muted);
}

.text-green {
  color: var(--color-primary-green);
}

.text-green-hover:hover {
  color: var(--color-primary-green-hover);
}

/* Transition Utilities - Transições padronizadas (200ms)
   Uso: Aplicar transições consistentes e rápidas */
.transition-fast {
  transition: var(--transition-all);
}

.transition-colors-fast {
  transition: var(--transition-colors);
}

.transition-transform-fast {
  transition: var(--transition-transform);
}

.transition-shadow-fast {
  transition: var(--transition-shadow);
}

/* Hover Transform Utilities - Transformações de hover padronizadas
   Uso: Elevação e escala consistentes */
.hover-lift {
  transition: var(--transition-transform);
}

.hover-lift:hover {
  transform: translateY(var(--hover-translate-y));
}

.hover-scale {
  transition: var(--transition-transform);
}

.hover-scale:hover {
  transform: scale(var(--hover-scale));
}

.hover-scale-button {
  transition: var(--transition-transform);
}

.hover-scale-button:hover {
  transform: scale(var(--hover-scale-button));
}

/* Shadow Utilities - Sombras do novo design system
   Uso: Aplicar sombras consistentes em cards e elementos */
.shadow-card {
  box-shadow: var(--card-shadow);
}

.shadow-card-hover {
  box-shadow: var(--card-shadow-hover);
}

.shadow-card-green {
  box-shadow: var(--card-shadow-green);
}

.shadow-card-green-hover {
  box-shadow: var(--card-shadow-green-hover);
}

/* Brand Color Utilities - Cores da marca
   Uso: Aplicar cores brand de forma consistente */
.text-brand-primary {
  color: var(--brand-green);
}

.text-brand-secondary {
  color: var(--brand-secondary);
}

.bg-brand-primary {
  background-color: var(--brand-green);
}

.bg-brand-secondary {
  background-color: var(--brand-secondary);
}

.border-brand-primary {
  border-color: var(--brand-green);
}

.border-brand-secondary {
  border-color: var(--brand-secondary);
}

@layer base {
  * {
    @apply border-border outline-ring/50;
  }
  
  /* Prevent horizontal overflow globally */
  html {
    overflow-x: hidden;
    max-width: 100vw;
  }
  
  body {
    @apply bg-white text-foreground;  /* Fundo branco */
    font-family: 'Space Grotesk', -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;
    overflow-x: hidden;
    max-width: 100vw;
    position: relative;
  }
  
  /* Ensure all images are responsive */
  img {
    max-width: 100%;
    height: auto;
  }
  
  /* Prevent text overflow */
  * {
    word-wrap: break-word;
    overflow-wrap: break-word;
  }
}

/* ========================================
   ACCESSIBILITY STYLES - VERDE MINIMALISTA
   Focus indicators verdes, reduced motion, ARIA
   ======================================== */

/* Skip Link - Visível apenas quando focado */
.skip-to-main {
  position: absolute;
  top: -40px;
  left: 6px;
  background: var(--color-primary-green);
  color: var(--color-primary-black);
  padding: 8px 16px;
  text-decoration: none;
  font-weight: bold;
  border-radius: 8px;
  z-index: 1000;
  transition: top 0.3s;
}

.skip-to-main:focus {
  top: 6px;
}

/* Focus Visible - Verde outline para navegação por teclado */
*:focus-visible {
  outline: 3px solid rgba(130, 247, 179, 0.6);
  outline-offset: 2px;
  border-radius: 4px;
}

/* Enhanced focus para elementos interativos com ring verde */
a:focus-visible,
button:focus-visible,
input:focus-visible,
textarea:focus-visible,
select:focus-visible,
[role="button"]:focus-visible,
[role="link"]:focus-visible,
[tabindex]:not([tabindex="-1"]):focus-visible {
  outline: none;
  box-shadow: 0 0 0 4px rgba(130, 247, 179, 0.2);
  transition: box-shadow var(--duration-fast) var(--ease-out);
}

/* Remove default focus para usuários de mouse */
*:focus:not(:focus-visible) {
  outline: none;
  box-shadow: none;
}

/* Keyboard Navigation Enhancements */
/* Ensure logical tab order and visible focus states */

/* Interactive elements should have minimum touch target size */
button,
a,
input,
select,
textarea,
[role="button"],
[role="link"],
[tabindex]:not([tabindex="-1"]) {
  min-height: 20px;
  min-width: 20px;
}

/* Exception for inline text links */
a:not([role="button"]):not(.btn):not(.button) {
  min-height: auto;
  min-width: auto;
}

/* Focus management for modal and overlay elements */
.modal-open {
  overflow: hidden;
}

.modal-backdrop {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1040;
}

/* Screen reader only content */
.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  margin: -1px;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  white-space: nowrap;
  border: 0;
}

.sr-only:focus,
.sr-only:active {
  position: static;
  width: auto;
  height: auto;
  padding: inherit;
  margin: inherit;
  overflow: visible;
  clip: auto;
  white-space: normal;
}

/* High contrast mode support */
@media (prefers-contrast: high) {
  :root {
    --color-primary-green: #00ff00;
    --color-primary-black: #000000;
    --color-bg-primary: #ffffff;
    --color-text-primary: #000000;
    --color-border-light: #000000;
  }
  
  button,
  input,
  select,
  textarea {
    border: 2px solid #000000;
  }
  
  a {
    text-decoration: underline;
  }
}

/* Focus trap for modals and dropdowns */
.focus-trap {
  position: relative;
}

.focus-trap:before,
.focus-trap:after {
  content: '';
  position: absolute;
  width: 1px;
  height: 1px;
  opacity: 0;
  pointer-events: none;
}

/* Keyboard navigation indicators */
.keyboard-navigation-active *:focus-visible {
  outline: 3px solid var(--color-primary-green);
  outline-offset: 2px;
}

/* Ensure proper heading hierarchy */
h1, h2, h3, h4, h5, h6 {
  font-weight: bold;
  margin-top: 0;
}

/* ARIA live regions */
.live-region {
  position: absolute;
  left: -10000px;
  width: 1px;
  height: 1px;
  overflow: hidden;
}

/* Focus management for single page applications */
[data-focus-guard] {
  width: 1px;
  height: 0px;
  padding: 0;
  overflow: hidden;
  position: fixed;
  top: 1px;
  left: 1px;
}

/* ========================================
   REDUCED MOTION SUPPORT - COMPREHENSIVE
   Suporte completo para prefers-reduced-motion
   Requirement 8.6: Configurar suporte a reduced motion
   ======================================== */

@media (prefers-reduced-motion: reduce) {
  /* Global animation and transition disabling */
  *,
  *::before,
  *::after {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
    scroll-behavior: auto !important;
  }
  
  /* Disable all CSS animations completely */
  [class*="animate-"],
  .animate-float,
  .animate-fade-in,
  .animate-slide-up,
  .animate-scale-in,
  .animate-bounce,
  .animate-pulse,
  .animate-spin,
  .animate-ping,
  .animate-morphing,
  .animate-pulse-glow,
  .animate-rotate-slow,
  .animate-bounce-subtle,
  .animate-float-delayed,
  .animate-float-reverse,
  .animate-slow-float,
  .animate-slow-float-delayed,
  .animate-marquee-left,
  .blog-card-entrance,
  .arrow-float-left,
  .arrow-float-right,
  .float-y,
  .float-y-1,
  .float-y-2,
  .float-y-3,
  .float-y-4,
  .float-y-5,
  .float-y-6 {
    animation: none !important;
    animation-duration: 0s !important;
    animation-delay: 0s !important;
  }
  
  /* Disable hover transforms that can cause motion sickness */
  .hover\:scale-105:hover,
  .hover\:scale-110:hover,
  .hover\:scale-\[1\.02\]:hover,
  .hover\:scale-\[1\.05\]:hover,
  .hover\:-translate-y-1:hover,
  .hover\:-translate-y-2:hover,
  .hover\:-translate-y-4:hover,
  .hover\:translate-x-1:hover,
  .hover\:translate-x-2:hover,
  .group-hover\:scale-105,
  .group-hover\:scale-110,
  .group-hover\:-translate-y-1,
  .group-hover\:translate-x-1,
  .hover-lift:hover,
  .hover-scale:hover,
  .hover-scale-button:hover {
    transform: none !important;
  }
  
  /* Disable all transitions while maintaining functionality */
  .transition-all,
  .transition-colors,
  .transition-transform,
  .transition-shadow,
  .transition-opacity,
  .transition-fast,
  .transition-colors-fast,
  .transition-transform-fast,
  .transition-shadow-fast {
    transition: none !important;
    transition-duration: 0s !important;
  }
  
  /* Disable specific component animations */
  .logo-hover-effect,
  .logo-glow::before,
  .logo-glow::after {
    animation: none !important;
    transition: none !important;
  }
  
  /* Disable carousel and slider animations */
  .carousel-slide,
  .slider-transition,
  .testimonial-slide {
    transition: none !important;
  }
  
  /* Disable loading and progress animations */
  .loading-spinner,
  .progress-bar-animated,
  .skeleton-loading {
    animation: none !important;
  }
  
  /* Disable parallax and scroll-based animations */
  .parallax-element,
  .scroll-reveal,
  .fade-in-on-scroll {
    transform: none !important;
    animation: none !important;
  }
  
  /* Maintain focus states without animation */
  *:focus-visible {
    transition: none !important;
  }
  
  /* Ensure buttons remain functional without hover animations */
  button:hover,
  .btn:hover,
  [role="button"]:hover {
    transform: none !important;
    transition: none !important;
  }
  
  /* Disable modal and overlay entrance animations */
  .modal-enter,
  .modal-exit,
  .overlay-fade,
  .dropdown-enter {
    animation: none !important;
    transition: none !important;
  }
  
  /* Disable slot machine animations */
  .slot-machine-container,
  .slot-reel {
    animation: none !important;
    transition: none !important;
  }
  
  /* Show only first slot item when motion is reduced */
  .slot-item:not(:first-child) {
    display: none;
  }
}

