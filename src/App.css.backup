/* Modern hero / spotlight styles */
.hero-bg {
  position: relative;
  overflow: hidden;
  background: radial-gradient(1200px 600px at 10% 20%, rgba(168,85,247,0.10), transparent 15%),
              radial-gradient(1000px 500px at 90% 80%, rgba(130,244,114,0.08), transparent 15%),
              linear-gradient(180deg, rgba(250,250,252,1) 0%, rgba(245,247,250,1) 100%);
}

.hero-container {
  position: relative;
  z-index: 10;
}

.blob {
  position: absolute;
  border-radius: 50%;
  filter: blur(36px);
  opacity: 0.85;
  mix-blend-mode: screen;
  transform-origin: center;
}

.blob--purple {
  width: 520px;
  height: 520px;
  background: radial-gradient(circle at 30% 30%, rgba(168,85,247,0.9), rgba(168,85,247,0.35) 40%, transparent 60%);
  right: -10%;
  top: -10%;
  animation: morphing 10s ease-in-out infinite;
}

.blob--green {
  width: 420px;
  height: 420px;
  background: radial-gradient(circle at 70% 70%, rgba(130,244,114,0.85), rgba(130,244,114,0.30) 45%, transparent 65%);
  left: -8%;
  bottom: -8%;
  animation: morphing 12s ease-in-out infinite reverse;
}

/* Generic colorful orbs for empty backgrounds */
.orb {
  position: absolute;
  border-radius: 9999px;
  filter: blur(28px);
  opacity: 0.28;
  pointer-events: none;
  mix-blend-mode: multiply;
  background-size: 200% 200%;
}
.orb--emerald {
  background-image: radial-gradient(circle at 30% 30%, rgba(16,185,129,0.65), rgba(16,185,129,0.15) 45%, transparent 65%);
}
.orb--teal {
  background-image: radial-gradient(circle at 60% 60%, rgba(45,212,191,0.55), rgba(45,212,191,0.15) 50%, transparent 70%);
}
.orb--purple {
  background-image: radial-gradient(circle at 50% 50%, rgba(168,85,247,0.50), rgba(168,85,247,0.12) 50%, transparent 70%);
