/* Site background dots pattern and subtle float animations */
.bg-dots {
  background-image: radial-gradient(#e5e7eb 1px, transparent 1px);
  background-size: 18px 18px;
}

@keyframes slow-float {
  0%, 100% { transform: translateY(0); }
  50% { transform: translateY(-8px); }
}
@keyframes slow-float-delayed {
  0%, 100% { transform: translateY(0); }
  50% { transform: translateY(10px); }
}

@keyframes bounce {
  0%, 100% { transform: translateY(0); }
  50% { transform: translateY(-10px); }
}

@keyframes pulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.8; }
}

.animate-slow-float { animation: slow-float 12s ease-in-out infinite; }
.animate-slow-float-delayed { animation: slow-float-delayed 14s ease-in-out infinite; }

/* Slot Machine Animations - Hero Section */

/* Keyframes for vertical slot spin animation */
@keyframes slot-spin-up {
  0% {
    transform: translate3d(0, 0, 0);
    opacity: 1;
  }
  100% {
    transform: translate3d(0, -100%, 0);
    opacity: 0.5;
  }
}

/* Slot Machine Container Styles */
.slot-machine-container {
  position: relative;
  background: rgba(255, 255, 255, 0.95);
  border: 1px solid rgba(217, 119, 87, 0.2);
  border-radius: 1rem;
  padding: 1rem 1.5rem;
  box-shadow: 0 4px 12px rgba(217, 119, 87, 0.15);
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  max-width: 220px;
  height: 60px;
  overflow: hidden;
  transition: all 0.3s ease;
  display: inline-flex;
  align-items: center;
  cursor: default;
}

/* Hover state for slot machine container */
.slot-machine-container:hover {
  box-shadow: 0 8px 24px rgba(217, 119, 87, 0.25);
  transform: scale(1.03);
  border-color: rgba(217, 119, 87, 0.4);
}

/* Inner container for icon and reel */
.slot-machine-inner {
  display: flex;
  align-items: center;
  gap: 0.75rem;
  width: 100%;
  height: 100%;
}

/* Icon styling */
.slot-icon {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  color: #d97757;
}

/* Slot reel container - creates the "window" effect */
.slot-reel-container {
  position: relative;
  height: 100%;
  flex: 1;
  overflow: hidden;
  display: flex;
  align-items: center;
}

/* Slot reel - the animated element */
.slot-reel {
  display: flex;
  flex-direction: column;
  transition: transform 0.6s cubic-bezier(0.4, 0, 0.2, 1);
  will-change: transform;
}

/* Individual slot items */
.slot-item {
  font-size: 1.25rem;
  font-weight: 600;
  color: #3d3d3a;
  white-space: nowrap;
  line-height: 60px;
  height: 60px;
  display: flex;
  align-items: center;
}

/* Gradient overlays for fade effect (top and bottom) */
.slot-machine-container::before,
.slot-machine-container::after {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  height: 15px;
  pointer-events: none;
  z-index: 10;
}

.slot-machine-container::before {
  top: 0;
  background: linear-gradient(to bottom, rgba(255, 255, 255, 0.95), transparent);
}

.slot-machine-container::after {
  bottom: 0;
  background: linear-gradient(to top, rgba(255, 255, 255, 0.95), transparent);
}

/* Remove will-change when paused for performance */
.slot-machine-container:hover .slot-reel {
  will-change: auto;
}

/* Duplicate reduced motion block removed - consolidated above */
  .slot-item:not(:first-child) {
    display: none;
  }
